WHITESPACE = _{ " " | "\t" }

expr       = { or_expr }
or_expr    = { and_expr ~ ( "||" ~ and_expr )* }
and_expr   = { not_expr ~ ( "&&" ~ not_expr )* }
not_expr   = { not* ~ atom }
not        = { "!" }
atom       = { word | "(" ~ expr ~ ")" }
word       = { (ASCII_ALPHANUMERIC | "_" | "-" | "%")+ }
